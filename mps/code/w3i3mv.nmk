  $(PFM)\$(VARIETY)\eventrep.obj \
  $(PFM)\$(VARIETY)\eventpro.obj $(PFM)\$(VARIETY)\table.obj \
  $(PFM)\$(VARIETY)\fmtpstst.obj $(TESTLIBOBJ) \
  $(PFM)\$(VARIETY)\mpsliban.obj $(SWOBJ)

