# We need a symbol for a non-empty definition with empty value
ifdef MPS_EMPTY
error "ic.gmk: MPS_EMPTY defined"
endif

CC = cc
CFLAGSCOMPILER = $(MPS_EMPTY)
CFLAGSDEBUG = -g
CFLAGSOPT = -O -g
CFLAGSOPTNODEBUG = -O

include comm.gmk
